<launch>

   <node pkg="image_view" type="image_view" name="image_view" respawn="false" output="log">
	<remap from="/image" to="/ORB_SLAM/Frame" />
	<param name="autosize" value="true"/>
	</node>

	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find ORB_SLAM)/Data/rviz.rviz" output="log">
   </node>
 	<node pkg="ORB_SLAM" type="ORB_SLAM" name="ORB_SLAM"  args="Data/ORBvoc.txt Data/Settings_AP.yaml" cwd="node" output="screen">
   </node>
   <node pkg="tf" type="static_transform_publisher" name="odom_map_broadcaster" args="0.175 0.05 0.175 0 0 0 1 base_footprint left_camera 30" />

</launch>

